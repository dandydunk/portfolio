<UserControl x:Class="WpfApplication1.Views.UserControlManageProjects"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApplication1"
             xmlns:viewmodels="clr-namespace:WpfApplication1.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" 
             d:DesignWidth="300">
    <StackPanel>
        <Button HorizontalAlignment="Left" Command="{Binding Path=NewProject}">NEW PROJECT</Button>
        <DataGrid></DataGrid>
    </StackPanel>
</UserControl>
